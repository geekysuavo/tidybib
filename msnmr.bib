@article{
   author = {Agnolet, Sara and Jaroszewski, Jerzy W. and Verpoorte, Robert and Staerk, Dan},
   title = {1H NMR-based metabolomics combined with HPLC-PDA-MS-SPE-NMR for investigation of standardized Ginkgo biloba preparations},
   journal = {Metabolomics},
   volume = {6},
   number = {Copyright (C) 2013 American Chemical Society (ACS). All Rights Reserved.},
   pages = {292-302},
   note = {CAPLUS AN 2010:636215(Journal)},
   abstract = {Com. prepns. of Ginkgo biloba are very complex mixts. prepd. from raw leaf exts. by a series of extn. and prepurifn. steps. The pharmacol. activity is attributed to a no. of flavonoid glycosides and unique terpene trilactones (TTLs), with largely uncharacterized pharmacol. profiles on targets involved in neurol. disorders. It is therefore important to complement existing targeted anal. methods for anal. of Ginkgo biloba prepns. with alternative technol. platforms for their comprehensive and global characterization. In this work, 1H NMR-based metabolomics and hyphenation of high-performance liq. chromatog., photo-diode array detection, mass spectrometry, solid-phase extn., and NMR spectroscopy (HPLC-PDA-MS-SPE-NMR) were used for investigation of 16 com. available prepns. of Ginkgo biloba. The standardized exts. originated from Denmark, Italy, Sweden, and United Kingdom, and the results show that 1H NMR spectra allow simultaneous assessment of the content as well as identity of flavonoid glycosides and TTLs based on a very simple sample-prepn. procedure consisting of extn., evapn. and reconstitution in acetone-d 6. Unexpected or unwanted ext. constituents were also easily identified in the 1H NMR spectra, which contrasts traditional methods that depend on UV absorption or MS ionizability and usually require availability of ref. stds. Automated integration of 1H NMR spectral segments (buckets or bins of 0.02 ppm width) provides relative distribution plots of TTLs based on their H-12 resonances. The present study shows that 1H NMR-based metabolomics is an attractive method for non-selective and comprehensive anal. of Ginkgo exts. [on SciFinder(R)]},
   keywords = {metabolomics Ginkgo herbal medicine HPLC PDA MS SPE NMR},
   year = {2010}
}

@article{
   author = {Annesley, Thomas M.},
   title = {Ion suppression in mass spectrometry},
   journal = {Clin. Chem. (Washington, DC, U. S.)},
   volume = {49},
   number = {7},
   pages = {1041-1044},
   note = {CAPLUS AN 2003:513126(Journal; General Review)},
   abstract = {A review. Mass spectrometry (MS) is being introduced into a large no. of clin. labs. It provides specificity because of its ability to monitor selected mass ions, sensitivity because of the enhanced signal-to-noise ratio, and speed because it can help avoid the need for intensive sample cleanup and long anal. times. However, MS is not without problems related to interference, esp. through ion suppression effects. Ion suppression results from the presence of less volatile compds. that can change the efficiency of droplet formation or droplet evapn., which in turn affects the amt. of charged ion in the gas phase that ultimately reaches the detector. This review discusses materials shown to cause ion suppression, including salts, ion-pairing agents, endogenous compds., drugs, metabolites, and proteins. Exptl. protocols for examg. ion suppression, which should include, at a min., signal recovery studies using specimen exts. with added analyte, are also discussed, and a more comprehensive approach is presented that uses postcolumn infusion of the analyte to evaluate protracted ionization effects. Finally, this review presents options for minimizing or correcting ion suppression, which include enhanced specimen cleanup, chromatog. changes, reagent modifications, and effective internal standardization. Whenever mass spectrometric assays are developed, ion suppression studies should be performed using expected physiol. concns. of the analyte under investigation. [on SciFinder(R)]},
   keywords = {review ion suppression mass spectrometry},
   year = {2003}
}

@article{
   author = {Atherton, Helen J. and Bailey, Nigel J. and Zhang, Wen and Taylor, John and Major, Hilary and Shockcor, John and Clarke, Kieran and Griffin, Julian L.},
   title = {A combined 1H-NMR spectroscopy- and mass spectrometry-based metabolomic study of the PPAR-α null mutant mouse defines profound systemic changes in metabolism linked to the metabolic syndrome},
   journal = {Physiol. Genomics},
   volume = {27},
   number = {2},
   pages = {178-186},
   note = {CAPLUS AN 2006:1213418(Journal; Online Computer File)},
   abstract = {The mobilization of triacylglycerides from storage in adipocytes to the liver is a vital response to the fasting state in mammalian metab. This is accompanied by a rapid translational activation of genes encoding mitochondrial, microsomal, and peroxisomal β-oxidn. in the liver, in part under the regulation of peroxisome proliferator-activated receptor-α (PPAR-α). A failure to express PPAR-α results in profound metabolic perturbations in muscle tissue as well as the liver. These changes represent a no. of deficits that accompany diabetes, dyslipidemia, and the metabolic syndrome. In this study, the metabolic role of PPAR-α has been investigated in heart, skeletal muscle, liver, and adipose tissue of PPAR-α null mice at 1 mo of age using metabolomics. To maximize the coverage of the metabolome in these tissues. 1H-NMR spectroscopy, magic angle spinning 1H-NMR spectroscopy, gas chromatog.-mass spectrometry, and liq. chromatog.-mass spectrometry were used to examine metabolites in aq. tissue exts. and intact tissue. The data were analyzed by the multivariate approaches of principal components anal. and partial least squares. Across all tissues, there was a profound decrease in glucose and a no. of amino acids, including glutamine and alanine, and an increase in lactate, demonstrating that a failure to express PPAR-α results in perturbations in glycolysis, the citric acid cycle, and gluconeogenesis. Furthermore, despite PPAR-α being weakly expressed in adipose tissue, a profound metabolic perturbation was detected in this tissue. [on SciFinder(R)]},
   keywords = {peroxisome proliferator activated receptor alpha metabolic syndrome},
   year = {2006}
}

