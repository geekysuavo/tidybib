@article{
   author = {Lin, Yiqing and Schiavo, Susan and Orjala, Jimmy and Vouros, Paul and Kautz, Roger},
   title = {Microscale LC-MS-NMR Platform Applied to the Identification of Active Cyanobacterial Metabolites},
   journal = {Anal. Chem. (Washington, DC, U. S.)},
   volume = {80},
   number = {Copyright (C) 2013 American Chemical Society (ACS). All Rights Reserved.},
   pages = {8045-8054},
   note = {CAPLUS AN 2008:1200627(Journal)},
   abstract = {An LC-MS-NMR platform is demonstrated, which combines two innovations in microscale anal., nanoSplitter LC-MS and microdroplet NMR, for the identification of unknown compds. found at low concns. in complex sample matrixes as frequently encountered in metabolomics or natural products discovery. The nanoSplitter provides the high sensitivity of nanoelectrospray MS while allowing 98% of the HPLC effluent from a large-bore LC column to be collected and concd. for NMR. Microdroplet NMR is a droplet microfluidic NMR loading method providing severalfold higher sample efficiency than conventional flow injection methods. Performing NMR offline from LC-UV-MS accommodates the disparity between MS and NMR in their sample mass and time requirements, as well as allowing NMR spectra to be requested retrospectively, after review of the LC-MS data. Interpretable 1D NMR spectra were obtained from analytes at the 200-ng level, in 1 h/well automated NMR data acquisitions. The system also showed excellent intra- and interdetector reproducibility with retention time RSD values less than 2% and sample recovery on the order of 93%. When applied to a cyanobacterial ext. showing antibacterial activity, the platform recognized several previously known metabolites, down to the 1% level, in a single 30-μg injection, and prioritized one unknown for further study. [on SciFinder(R)]},
   keywords = {microscale liq chromatog mass spectrometry NMR platform cyanobacterial metabolite},
   year = {2008}
}

