choice
number},
   year = {2004}
}

@article{
   author = {Xu, Yun and Correa, Elon and Goodacre, Royston},
   title = {Integrating multiple analytical platforms and chemometrics for comprehensive metabolic profiling: application to meat spoilage detection},
   journal = {Anal. Bioanal. Chem.},
   volume = {405},
   number = {15},
   pages = {5063-5074},
   note = {Copyright (C) 2014 American Chemical Society (ACS). All Rights Reserved.
CAPLUS AN 2013:426424(Journal; Online Computer File)
10.1007/s00216-013-6884-3},
   abstract = {Untargeted metabolic profiling has become a common approach to attempt to understand biol. systems. However, due to the large chem. diversity in the metabolites it is generally necessary to employ multiple anal. platforms so as to encompass a wide range of metabolites. Thus it is beneficial to find chemometrics approaches which can effectively integrate data generated from multiple platforms and ideally combine the strength of each platform and overcome their inherent weaknesses; most pertinent is with respect to limited chemistries. We have reported a few studies using untargeted metabolic profiling techniques to monitor the natural spoilage process in pork and also to detect specific metabolites assocd. with contaminations with the pathogen Salmonella typhimurium. One method used was to analyze the volatile org. compds. (VoCs) generated throughout the spoilage process while the other was to analyze the sol. small mol. metabolites (SMM) extd. from the microbial community, as well as from the surface of the spoiled/contaminated meat. In this study, we exploit multi-block principal component anal. (MB-PCA) and multi-block partial least squares (MB-PLS) to combine the VoCs and SMM data together and compare the results obtained by analyzing each data set individually. We show that by combining the two data sets and applying appropriate chemometrics, a model with much better prediction and importantly with improved interpretability was obtained. The MB-PCA model was able to combine the strength of both platforms together and generated a model with high consistency with the biol. expectations, despite its unsupervised nature. MB-PLS models also achieved the best over-all performance in modeling the spoilage progression and discriminating the naturally spoiled samples and the pathogen contaminated samples. Correlation anal. and Bayesian network anal. were also performed to elucidate which metabolites were correlated strongly in the two data sets and such information could add addnl. information in understanding the meat spoilage process. [on SciFinder(R)]},
   keywords = {chemometric metab metabolite profiling meat spoilage},
   year = {2013}
}

@article{
   author = {Yde, Christian Clement and Clausen, Morten Rahr and Ditlev, Ditte Bruun and Lillefosse, Haldis and Madsen, Lise and Kristiansen, Karsten and Liaset, Bjoern and Bertram, Hanne Christine},
   title = {Multi-block PCA and multi-compartmental study of the metabolic responses to intake of hydrolysed versus intact casein in C57BL/6J mice by NMR-based metabolomics},
   journal = {Metabolomics},
   volume = {Ahead of Print},
   pages = {Ahead of Print},
   note = {CAPLUS AN 2014:148056(Journal)},
   abstract = {Recently we showed that exchanging intact casein with extensively hydrolyzed casein in Western diets prevented diet-induced obesity in obesity-prone C57BL/6J mice. To gain further insight into the underlying mechanisms for the metabolic alterations induced by intake of hydrolyzed casein, we performed an exploratory investigation using proton NMR spectroscopy, multi-block PCA (MBPCA) and a multi-compartment model including analyses of plasma, urine, faeces and tissue samples from mice fed diets with intact or hydrolyzed casein and 16 or 32 energy% protein. The MBPCA superscores showed a clear sepn. between samples from mice fed intact and hydrolyzed casein diets, resp. Block loadings revealed that fecal fat content was higher, and tissue and plasma lipid levels were lower in mice fed hydrolyzed casein diets compared with mice fed intact casein. Amino acid metab. was also altered by dietary protein form, and levels of branched-chain amino acids were higher in faeces and urine and lower in plasma and spleen in mice fed hydrolyzed protein. Moreover, hepatic levels of the sulfur-contg. metabolites taurine and glutathione were increased in mice fed hydrolyzed casein, and hepatic glycogen amt. was increased in mice fed hydrolyzed casein. In contrast, the levels of glucose and its metabolite lactate were reduced in faeces, liver and plasma. Taken together, NMR-based metabolomic analyses indicated that pathways within lipid, amino acid and carbohydrate metab. were altered by intake of hydrolyzed casein, and that these alterations are likely to be underlying mechanisms for the obsd. prevention against diet-induced obesity assocd. with hydrolyzed casein intake. [on SciFinder(R)]},
   year = {2014}
}

@article{
   author = {Yoo, Byong Chul and Kong, Sun-Young and Jang, Sang-Geun and Kim, Kyung-Hee and Ahn, Sun- A. and Park, Weon-Seo and Park, Sohee and Yun, Tak and Eom, Hyeon-Seok},
   title = {Identification of hypoxanthine as a urine marker for non-Hodgkin lymphoma by low-mass-ion profiling},
   journal = {BMC Cancer},
   volume = {10},
   pages = {55},
   note = {MEDLINE AN 2010193856(Journal; Article; (JOURNAL ARTICLE); (RESEARCH SUPPORT, NON-U.S. GOV'T))},
   abstract = {BACKGROUND: Non-Hodgkin lymphoma (NHL) is a hematologic malignancy for which good diagnostic markers are lacking. Despite continued improvement in our understanding of NHL, efforts to identify diagnostic markers have yielded dismal results. Here, we translated low-mass-ion information in urine samples from patients with NHL into a diagnostic marker. METHODS: To minimize experimental error, we tested variable parameters before MALDI-TOF analysis of low-mass ions in urine. Urine from 30 controls and 30 NHL patients was analyzed as a training set for NHL prediction. All individual peak areas were normalized to total area up to 1000 m/z. The training set analysis was repeated four times. Low-mass peaks that were not affected by changes in experimental conditions were collected using MarkerView software. Human Metabolome Database (HMDB) searches and ESI LC-MS/MS analyses were used to identify low-mass ions that exhibited differential patterns in control and NHL urines. Identified low-mass ions were validated in a blinded fashion in 95 controls and 66 NHL urines to determine their ability to discriminate NHL patients from controls. RESULTS: The 30 highest-ranking low-mass-ion peaks were selected from the 60-urine training set, and three low-mass-ion peaks with high intensity were selected for identification. Of these, a 137.08-m/z ion showed lower mass-peak intensity in urines of NHL patients, a result that was validated in a 161-urine blind validation set (95 controls and 66 NHL urines). The 130.08-m/z ion was identified from HMDB searches and ESI LC-MS/MS analyses as hypoxanthine (HX). The HX concentration in urines of NHL patients was significantly decreased (P < 0.001) and was correlated with the mass-peak area of the 137.08-m/z ion. At an HX concentration cutoff of 17.4 microM, sensitivity and specificity were 79.2% and 78.4%, respectively. CONCLUSIONS: The present study represents a good example of low-mass-ion profiling in the setting of disease screening using urine. This technique can be a powerful non-invasive diagnostic tool with high sensitivity and specificity for NHL screening. Furthermore, HX identified in the study may be a useful single urine marker for NHL screening.[on SciFinder (R)]},
   year = {2010}
}

