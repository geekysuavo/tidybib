@article{
   author = {Cech, Nadja B. and Enke, Christie G.},
   title = {Practical implications of some recent studies in electrospray ionization fundamentals},
   journal = {Mass Spectrom. Rev.},
   volume = {20},
   number = {6},
   pages = {362-387},
   note = {CAPLUS AN 2002:467607(Journal; General Review)},
   abstract = {A review. In accomplishing successful electrospray ionization analyses, it is imperative to have an understanding of the effects of variables such as analyte structure, instrumental parameters, and soln. compn. Some fundamental studies of the ESI process that are relevant to these issues are reviewed. Analyte chargeability and surface activity are related to ESI response, and accessible parameters such as nonpolar surface area and reversed phase HPLC retention time can be used to predict relative ESI response. A description is presented of how derivatizing agents can be used to maximize or enable ESI response by improving the chargeability or hydrophobicity of ESI analytes. Limiting factors in the ESI calibration curve are discussed. At high concns., these factors include droplet surface area and excess charge concn., whereas at low concns. ion transmission becomes an issue, and chem. interference can also be limiting. Stable and reproducible nonpneumatic ESI operation depends on the ability to balance a no. of parameters, including applied voltage and soln. surface tension, flow rate, and cond. The authors discuss how changing these parameters can shift the mode of ESI operation from stable to unstable, and how current-voltage curves can be used to characterize the mode of ESI operation. The characteristics of the ideal ESI solvent, including surface tension and cond. requirements, are discussed. Anal. in the pos. ion mode can be accomplished with acidified MeOH/H2O solns., but neg. ion mode anal. necessitates special constituents that suppress corona discharge and facilitate the prodn. of stable neg. ions. [on SciFinder(R)]},
   keywords = {review electrospray ionization mass spectrometry},
   year = {2002}
}

