}

@article{
   author = {Barding, Gregory A. and Beni, Szabolcs and Fukao, Takeshi and Bailey-Serres, Julia and Larive, Cynthia K.},
   title = {Comparison of GC-MS and NMR for Metabolite Profiling of Rice Subjected to Submergence Stress},
   journal = {J. Proteome Res.},
   volume = {12},
   number = {2},
   pages = {898-909},
   note = {CAPLUS AN 2012:1762349(Journal; Online Computer File)},
   abstract = {Natural disasters such as drought, extreme temps., and flooding can severely impact crop prodn. Understanding the metabolic response of crops threatened with these disasters provides insights into biol. response mechanisms that can influence survival. A comparative anal. of GC-MS and 1H NMR results was conducted for wild-type and tolerant rice varieties stressed by up to 3 days of submergence and allowed 1 day of postsubmergence recovery. Most metabolomics studies are conducted using a single anal. platform. Each platform, however, has inherent advantages and disadvantages that can influence the anal. coverage of the metabolome. A more thorough anal. of the plant stress response was possible through the use of both 1H NMR and GC-MS. Several metabolites, such as S-Me methionine and the dipeptide alanylglycine, were only detected and quantified by 1H NMR. The high dynamic range of NMR, as compared with that of the GC-TOF-MS used provided broad coverage of the metabolome in a single expt. The sensitivity of GC-MS facilitated the quantitation of sugars, org. acids, and amino acids, some of which were not detected by NMR, and provided addnl. insights into the regulation of the TCA cycle. The combined metabolic information provided by 1H NMR and GC-MS was essential for understanding the complex biochem. and mol. response of rice plants to submergence. [on SciFinder(R)]},
   keywords = {gas chromatog mass spectrometry NMR metabolite rice submergence stress},
   year = {2013}
}

