@article{
   author = {Westerhuis, Johan A. and Kourti, Theodora and Macgregor, John F.},
   title = {Analysis of multiblock and hierarchical PCA and PLS models},
   journal = {J. Chemom.},
   volume = {12},
   number = {5},
   pages = {301-321},
   note = {Copyright (C) 2014 American Chemical Society (ACS). All Rights Reserved.
CAPLUS AN 1998:714905(Journal; General Review)
10.1002/(SICI)1099-128X(199809/10)12:5<301::AID-CEM515>3.0.CO;2-S},
   abstract = {A review, with 25 refs., is given on several algorithms developed to deal with multiple-block data using different notation, under names such as hierarchical PCA (HPCA), consensus PCA (CPCA), hierarchical PLS (HPLS) and multiblock PLS (MBPLS); and comparison from a theor. view point using a common notation. Multiblock and hierarchical PCA and PLS methods were proposed in the recent literature to improve the interpretability of multivariate models. They were used in cases where the no. of variables is large and addnl. information is available for blocking the variables into conceptually meaningful blocks. The authors compare these methods from a theor. or algorithmic viewpoint using a common notation and illustrate their differences with several case studies. Undesirable properties of some of these methods, such as convergence problems or loss of data information due to deflation procedures, are pointed out and cor. where possible. Objective function of the hierarchical PCA and hierarchical PLS methods is not clear and the corresponding algorithms may converge to different solns. depending on the initial guess of the super score. Also the results of CPCA and MBPLS can be calcd. from the std. PCA and PLS methods when the same variable scalings are applied for these methods. The std. PCA and PLS methods require less computation and give better estn. of the scores in the case of missing data. It is therefore recommended that in cases where the variables can be sepd. into meaningful blocks, the std. PCA and PLS methods be used to build the models and then the wts. and loadings of the individual blocks and super block and the percentage variation explained in each block be calcd. [on SciFinder(R)]},
   keywords = {review multiblock hierarchical PCA PLS model},
   year = {1998}
}

@article{
   author = {Whitfield, Phillip D. and German, Alexander J. and Noble, Peter-John M.},
   title = {Metabolomics: An emerging post-genomic tool for nutrition},
   journal = {Br. J. Nutr.},
   volume = {92},
   number = {4},
   pages = {549-555},
   note = {CAPLUS AN 2004:894913(Journal; General Review)},
   abstract = {A review. The post-genomic era has been driven by the development of technologies that allow the function of cells and whole organisms to be explored at the mol. level. Metabolomics is concerned with the measurement of global sets of low-mol.-wt. metabolites. Metabolite profiles of body fluids or tissues can be regarded as important indicators of physiol. or pathol. states. Such profiles may provide a more comprehensive view of cellular control mechanisms in man and animals, and raise the possibility of identifying surrogate markers of disease. Metabolomic approaches use anal. techniques such as NMR spectroscopy and MS to measure populations of low-mol.-wt. metabolites in biol. samples. Advanced statistical and bioinformatic tools are then used to maximize the recovery of information and interpret the large data sets generated. Metabolomics has already been used to study toxicol. mechanisms and disease processes and offers enormous potential for investigating complex relationships between nutrition and metab. Examples include the metab. of dietary substrates, drug-induced disturbances of lipid metabolites in type 2 diabetes mellitus, and therapeutic effects of vitamin supplementation in the treatment of chronic metabolic disorders. [on SciFinder(R)]},
   keywords = {review nutrition metabolomic methodol},
   year = {2004}
}

