   pages = {465-471},
   note = {CAPLUS AN 2005:503375(Journal)},
   abstract = {1H NMR and HPLC-MS were used to generate metabolite fingerprints for the metabonomic anal. of urine obtained from both male and female Zucker obese (fa/fa) rats, used as a model of type II diabetes, and normal male Wistar-derived animals. The resulting data were subjected to chemometric anal. (principal components anal. and partial least squares discriminant anal.) to investigate the effects of strain, diurnal variation is strain, diurnal variation and gender and gender on metabolite profiles. In the case of strain, 1H NMR spectroscopic anal. revealed increased taurine, hippurate and formate and decreased betaine, α-ketoglutarate, succinate and acetate in samples from Zucker-obese compared to Wistar-derived rats. HPLC-MS anal. detected increased hippurate and ions at m/z 255.0640 and 285.0770 in pos., and 245.0122 and 261.0065 in neg. electrospray ionization (ESI), resp., for the Zucker obese samples. Both techniques enable the detection of diurnal variation in the urine of male and female Zucker rats, marked by increases in taurine, creatinine, allantoin and α-ketoglutarate by 1H NMR, and ions at m/z 285.0753, 291.0536 and 297.1492 (pos. ESI) and 461.1939 (neg. ESI) using HPLC-MS, in the evening samples. Differences between male and female Zucker rats were also obsd. Compared to samples from male rats hippurate, succinate, α-ketoglutarate and dimethylglycine (1H NMR) were elevated in the urine of female animals together with ions at, e.g., m/z 431.1047, 325.0655, 271.0635 and 447.0946 (pos. ESI) and m/z 815.5495 and 459.0985 (neg. ESI) by HPLC-MS. Both anal. techniques used in this study were able to detect differences between normal and Zucker obese rats, which may provide markers of metabolic disease. [on SciFinder(R)]},
   keywords = {NMR spectrometry HPLC MS metabonomic urine obese Zucker Wistar},
   year = {2005}
}

@article{
   author = {Wishart, David S.},
   title = {Applications of metabolomics in drug discovery and development},
   journal = {Drugs R&D},
   volume = {9},
   number = {5},
   pages = {307-322},
   note = {CAPLUS AN 2008:1216026(Journal; General Review)},
   abstract = {A review. Metabolomics is a relatively new field of 'omics' technol. that is primarily concerned with the global or system-wide characterization of small mol. metabolites using technologies such as NMR, liq. chromatog. and/or mass spectrometry. Its unique focus on small mols. and the physiol. effects of small mols. aligns the field of metabolomics very closely with the aims and interests of many researchers in the pharmaceutical industry. Because of its conceptual and tech. overlap with many aspects of pharmaceutical research, metabolomics is now finding applications that span almost the full length of the drug discovery and development pipeline, from lead compd. discovery to post-approval drug surveillance. This review explores some of the most interesting or significant applications of metabolomics as they relate to pharmaceutical research and development. Specific examples are given that show how metabolomics can be used to facilitate lead compd. discovery, to improve biomarker identification (for monitoring disease status and drug efficacy) and to monitor drug metab. and toxicity. Other applications are also discussed, including the use of metabolomics to facilitate clin. trial testing and to improve post-approval drug monitoring. These examples show that metabolomics potentially offer drug researchers and drug regulators an effective, inexpensive route to addressing many of the riskier or more expensive issues assocd. with the discovery, development and monitoring of drug products. [on SciFinder(R)]},
   keywords = {review metabolomics drug development discovery metab},
   year = {2008}
}

