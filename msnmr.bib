   pages = {10398-10407},
   note = {CAPLUS AN 2013:1532955(Journal; Online Computer File)},
   abstract = {A metabolomic 1H-NMR and UPLC-MS anal. examd. the post-harvest processing of Astragalus membranaceus (herbal medicine) roots, with focus on the peeling procedure used. Principal component anal. (PCA) score plots from the NMR and MS data showed clear sepn. between peeled and nonpeeled Astragalus roots. Peeled roots showed significant losses of several primary metabolites, including acetate, alanine, arginine, caprate, fumarate, glutamate, histidine, N-acetylaspartate, malate, proline, sucrose, trigonelline, and valine. The peeled roots contained higher levels of asparagine, aspartate, and xylose, which are xylem-related compds., and formate, which is produced in response to wound stress incurred during post-harvest peeling processing. The levels of isoflavonoids and astragalosides were decreased in peeled Astragalus root. Thus, metabolite profiling by combined 1H-NMR and UPLC-MS analyses can be used to evaluate peeling procedures used in the post-harvest processing of herbal medicines. [on SciFinder(R)]},
   keywords = {pharmaceutical analysis Astragalus root peeling metabolomic profile HPLC MS},
   year = {2013}
}

@article{
