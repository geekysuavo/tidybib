@article{
   author = {Beckonert, Olaf and Keun, Hector C. and Ebbels, Timothy M. D. and Bundy, Jacob and Holmes, Elaine and Lindon, John C. and Nicholson, Jeremy K.},
   title = {Metabolic profiling, metabolomic and metabonomic procedures for NMR spectroscopy of urine, plasma, serum and tissue extracts},
   journal = {Nat. Protoc.},
   volume = {2},
   number = {11},
   pages = {2692-2703},
   note = {CAPLUS AN 2007:1306038(Journal; Online Computer File)},
   abstract = {Metabolic profiling, metabolomic and metabonomic studies mainly involve the multicomponent anal. of biol. fluids, tissue and cell exts. using NMR spectroscopy and/or mass spectrometry (MS). The authors summarize the main NMR spectroscopic applications in modern metabolic research, and provide detailed protocols for biofluid (urine, serum/plasma) and tissue sample collection and prepn., including the extn. of polar and lipophilic metabolites from tissues. 1H NMR spectroscopic techniques such as std. 1D spectroscopy, relaxation-edited, diffusion-edited and 2D J-resolved pulse sequences are widely used at the anal. stage to monitor different groups of metabolites and are described here. They are often followed by more detailed statistical anal. or addnl. 2D NMR anal. for biomarker discovery. The std. acquisition time per sample is 4-5 min for a simple 1D spectrum, and both prepn. and anal. can be automated to allow application to high-throughput screening for clin. diagnostic and toxicol. studies, as well as mol. phenotyping and functional genomics. [on SciFinder(R)]},
   keywords = {metabolomic metabolic profiling NMR spectroscopy urine plasma serum tissue
metabonomic metabolic profiling NMR spectroscopy urine plasma serum tissue},
   year = {2007}
}

@article{
   author = {Beltran, Antoni and Suarez, Manuel and Rodriguez, Miguel A. and Vinaixa, Maria and Samino, Sara and Arola, Lluis and Correig, Xavier and Yanes, Oscar},
   title = {Assessment of Compatibility between Extraction Methods for NMR- and LC/MS-Based Metabolomics},
   journal = {Anal. Chem. (Washington, DC, U. S.)},
   volume = {84},
   number = {14},
   pages = {5838-5844},
   note = {CAPLUS AN 2012:853656(Journal; Online Computer File)},
   abstract = {Because of the wide range of chem. and structurally diverse metabolites, efforts to survey the complete metabolome rely on the implementation of multi-platform approaches based on NMR and mass spectrometry (MS). Sample prepn. disparities between NMR and MS, however, may limit the anal. of the same samples by both platforms. Specifically, deuterated solvents used in NMR strategies can complicate LC/MS anal. as a result of potential mass shifts, whereas acidic solns. typically used in LC/MS methods to enhance ionization of metabolites can severely affect reproducibility of NMR measurements. These intrinsically different sample prepn. requirements result in the application of different procedures for metabolite extn., which involve addnl. sample and unwanted variability. To address this issue, the authors investigated 12 extn. protocols in liver tissue involving different aq./org. solvents and temps. that may satisfy the requirements for both NMR and LC/MS simultaneously. The authors found that deuterium exchange did not affect LC/MS results, enabling the measurement of metabolites by NMR and, subsequently, the direct anal. of the same samples by using LC/MS with no need for solvent exchange. Moreover, the results show that the choice of solvents rather than the temp. detd. the extn. efficiencies of metabolites, a combination of methanol/chloroform/water and methanol/water being the extn. methods that best complement NMR and LC/MS anal. for metabolomic studies. [on SciFinder(R)]},
   keywords = {extn solvent NMR liq chromatog mass spectrometry metabolome},
   year = {2012}
}

@article{
   author = {Bertram, Hanne C. and Knudsen, Knud E. Bach and Serena, Anja and Malmendal, Anders and Nielsen, Niels Chr and Frette, Xavier C. and Andersen, Henrik J.},
   title = {NMR-based metabonomic studies reveal changes in the biochemical profile of plasma and urine from pigs fed high-fiber rye bread},
   journal = {Br. J. Nutr.},
   volume = {95},
   number = {Copyright (C) 2013 American Chemical Society (ACS). All Rights Reserved.},
   pages = {955-962},
   note = {CAPLUS AN 2006:497657(Journal)},
   abstract = {This study presents an NMR-based metabonomic approach to elucidate the overall endogenous biochem. effects of a wholegrain diet. Two diets with similar levels of dietary fiber and macronutrients, but with contrasting levels of wholegrain ingredients, were prepd. from wholegrain rye (wholegrain diet (WGD)) and non-wholegrain wheat (non-wholegrain diet (NWD)) and fed to four pigs in a crossover design. Plasma samples were collected after 7 d on each diet, and 1H NMR spectra were acquired on these. Partial least squares regression discriminant anal. (PLS-DA) on spectra obtained for plasma samples revealed that the spectral region at 3.25 ppm dominates the differentiation between the two diets, as the WGD is assocd. with higher spectral intensity in this region. Spiking expts. and LC-MS analyses of the plasma verified that this spectral difference could be ascribed to a significantly higher content of betaine in WGD plasma samples compared with NWD samples. In an identical study with the same diets, urine samples were collected, and 1H NMR spectra were acquired on these. PLS-DA on spectra obtained for urine samples revealed changes in the intensities of spectral regions, which could be ascribed to differences in the content of betaine and creatine/creatinine between the two diets, and LC-MS analyses verified a significantly lower content of creatinine in WGD urine samples compared with NWD urine samples. In conclusion, using an explorative approach, the present studies disclosed biochem. effects of a wholegrain diet on plasma betaine content and excretion of betaine and creatinine. [on SciFinder(R)]},
   keywords = {NMR metabonomic plasma urine pig fed fiber rye bread},
   year = {2006}
}

@article{
   author = {Birungi, Grace and Chen, Sheryl Mei-Jie and Loy, Boon-Pheng and Ng, Mah-Lee and Li, Sam Fong-Yau},
   title = {Metabolomics Approach for Investigation of Effects of Dengue Virus Infection Using the EA.hy926 Cell Line},
   journal = {J. Proteome Res.},
   volume = {9},
   number = {Copyright (C) 2013 American Chemical Society (ACS). All Rights Reserved.},
   pages = {6523-6534},
   note = {CAPLUS AN 2010:1384359(Journal)},
   abstract = {This paper describes a multiplatform anal. approach combining proton NMR (1H NMR) spectroscopy and mass spectrometry (MS), together with pattern recognition tools in a metabolomic study used to study the effects of dengue virus infection. The four serotypes of dengue, DEN-1, DEN-2, DEN-3, and DEN-4, were inoculated into the EA.hy926 cell line, which was then incubated for various time intervals. Principal component anal. (PCA) of the 1H NMR and MS data revealed metabolic profile patterns or fingerprint patterns that can be attributed to specific virus serotypes. Distinct effects of infection by each serotype were demonstrated, and these differences were attributed to changes in levels of metabolites (including amino acids, dicarboxylic acids, fatty acids, and org. acids related to the tricarboxylic acid (TCA) cycle). The study demonstrated application of metabolomics to improve understanding of the effect of dengue infection on endothelial cells' metabolome. [on SciFinder(R)]},
   keywords = {metabolomics dengue virus infection vessel endothelium},
   year = {2010}
}

@article{
   author = {Bonfiglio, R. and King, R. C. and Olah, T. V. and Merkle, K.},
   title = {The effects of sample preparation methods on the variability of the electrospray ionization response for model drug compounds},
   journal = {Rapid Commun Mass Spectrom},
   volume = {13},
   number = {12},
   pages = {1175-1185},
   note = {Bonfiglio
King
Olah
Merkle
Journal article
Rapid communications in mass spectrometry : RCM
Rapid Commun Mass Spectrom. 1999 Jun;13(12):1175-1185.},
   abstract = {A post-column infusion system was developed in order to analyze suppression of electrospray ionization (ESI) tandem mass spectrometry response in the presence of endogenous plasma interferences. By enabling direct detection of these interfering components, this experimental system was used to analyze the ability of several common extraction procedures to remove endogenous plasma components that cause changes in the ESI response of model drug substances. Methyl-t-butyl ether (MTBE) liquid-liquid, Oasis and Empore solid-phase, and acetonitrile (ACN) protein precipitation sample preparation methods were tested using the post-column infusion system. In all cases, ACN protein precipitation samples showed the greatest amount of ESI response suppression while liquid-liquid extracts demonstrated the least. In addition, the three test compounds, phenacetin, caffeine, and a representative Merck compound, demonstrated that ESI response suppression is compound dependent. Suppression was greatest with caffeine, the most polar analyte, and the smallest for the Merck compound, the least polar analyte. Copyright 1999 John Wiley & Sons, Ltd.},
   year = {1999}
}

@article{
   author = {Booth, Sean C. and Workentine, Matthew L. and Wen, Jing and Shaykhutdinov, Rustem and Vogel, Hans J. and Ceri, Howard and Turner, Raymond J. and Weljie, Aalim M.},
   title = {Differences in Metabolism between the Biofilm and Planktonic Response to Metal Stress},
   journal = {J. Proteome Res.},
   volume = {10},
   number = {Copyright (C) 2013 American Chemical Society (ACS). All Rights Reserved.},
   pages = {3190-3199},
   note = {CAPLUS AN 2011:677590(Journal; Online Computer File)},
   abstract = {Bacterial biofilms are known to withstand the effects of toxic metals better than planktonic cultures of the same species. This phenomenon has been attributed to many features of the sessile lifestyle not present in free-swimming populations, but the contribution of intracellular metab. has not been previously examd. Here, we use a combined GC-MS and 1H NMR metabolomic approach to quantify whole-cell metab. in biofilm and planktonic cultures of the multimetal resistant bacterium Pseudomonas fluorescens exposed to copper ions. Metabolic changes in response to metal exposure were found to be significantly different in biofilms compared to planktonic cultures. Planktonic metab. indicated an oxidative stress response that was characterized by changes to the TCA cycle, glycolysis, pyruvate and nicotinate and niacotinamide metab. Similar metabolic changes were not obsd. in biofilms, which were instead dominated by shifts in exopolysaccharide related metab. suggesting that metal stress in biofilms induces a protective response rather than the reactive changes obsd. for the planktonic cells. From these results, we conclude that differential metabolic shifts play a role in biofilm-specific multimetal resistance and tolerance. An altered metabolic response to metal toxicity represents a novel addn. to a growing list of biofilm-specific mechanisms to resist environmental stress. [on SciFinder(R)]},
   keywords = {metab biofilm planktonic metal copper stress resistance adaptation},
   year = {2011}
}

