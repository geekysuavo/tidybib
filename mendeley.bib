journal = {Concepts in Magnetic Resonance},
keywords = {analog filter,analog to digital,antialiasing,baseline,digital filter,filter coefficient,finite impulse response,group delay,high resolution,impulse response,infi-,nite impulse response,nmr spectroscopy,nonrecursive filter,nyquist,oversampling,phase delay,quantization error,recursive filter,step response},
number = {2},
pages = {164--176},
title = {{Application of real time digital filters in NMR spectroscopy}},
