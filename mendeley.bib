journal = {Journal of Biomolecular NMR},
keywords = {Peak picking,Resonance assignment,Spin system,Wavelet},
number = {2},
pages = {75--86},
pmid = {24748536},
title = {{An automated framework for NMR resonance assignment through simultaneous slice picking and spin system forming}},
volume = {59},
year = {2014}
}
