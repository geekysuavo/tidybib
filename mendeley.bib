keywords = {CE-MS,Chemical analysis,Flux,GC-MS,Isotopomer analysis,LC-MS,Mass spectrometry,Metabolite identification,Metabolite profiling,Metabolomics,Method validation,Minimum reporting standards,NMR,Nuclear magnetic resonance,Quality control},
