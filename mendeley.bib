journal = {Biophysical Journal},
number = {5},
pages = {2422--2430},
pmid = {11325741},
title = {{The conformation of serum albumin in solution: a combined phosphorescence depolarization-hydrodynamic modeling study}},
volume = {80},
year = {2001}
}
