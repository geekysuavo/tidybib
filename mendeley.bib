issn = {15733149},
journal = {International Journal of Peptide Research and Therapeutics},
keywords = {Difficult sequences,In situ neutralization,Ion-spray mass spectrometry,Solid phase peptide synthesis (SPPS)},
number = {1-2},
pages = {31--44},
