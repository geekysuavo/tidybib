issn = {09252738},
journal = {Journal of Biomolecular NMR},
keywords = {Compressed sensing,FM reconstruction,Iterative soft thresholding,Maximum entropy reconstruction,Nuclear magnetic resonance,Sparse sampling,Spectra reconstruction},
number = {4},
pages = {315--327},
pmid = {22331404},
title = {{Application of iterative soft thresholding for fast reconstruction of NMR data non-uniformly sampled with multidimensional Poisson Gap scheduling}},
volume = {52},
year = {2012}
}
