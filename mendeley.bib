title = {{The allosteric communication pathways in KIX domain of CBP}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3761601\&tool=pmcentrez\&rendertype=abstract},
volume = {110},
year = {2013}
}
