keywords = {Allosteric Regulation,CREB-Binding Protein,CREB-Binding Protein: chemistry,CREB-Binding Protein: metabolism,Models, Molecular,Molecular,Molecular Dynamics Simulation,Myeloid-Lymphoid Leukemia Protein,Myeloid-Lymphoid Leukemia Protein: chemistry,Myeloid-Lymphoid Leukemia Protein: metabolism,Phosphorylation},
number = {35},
pages = {14237--42},
